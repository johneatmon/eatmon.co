---
import * as Colophon from "@components/Colophon.md"
import PostLayout from "@layouts/PostLayout.astro"

export const prerender = true

const { title, description, published, modified } = Colophon.frontmatter
---

<PostLayout
	title={title}
	description={description}
	published={published}
	modified={modified}
	set:html={Colophon.compiledContent()}
/>
